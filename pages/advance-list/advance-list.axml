<view class="page">
  <view class="m-tabs-pane">
    <view class="m-tabs">
      <view class="m-tabs-ink-bar" style="width:{{itemWidth}}px ;transform: translate3d({{inkBar}}px, 0px, 0px);"></view>
      <view class="m-tabs-item {{currentTabIndex==0?'m-tabs-item-active':''}}" data-index="0" onTap="onSelectTab">
        待我审批的
      </view>
      <view class="m-tabs-item {{currentTabIndex==1?'m-tabs-item-active':''}}" data-index="1"  onTap="onSelectTab">
        我已审批的
      </view>
    </view>
  </view>
  
  <view class="m-tabs-con">
      <view class="m-tab-pane" a:if="{{currentTabIndex == 0}}">
        <view class="m-adv-item" a:for="{{list}}">
          <navigator url="/pages/advance/advance?id={{item.id}}">
            <view class="m-adv-name">{{item.processTitle}}</view>
            <view class="m-adv-date">申请时间:{{item.date}}</view>
            <view class="m-adv-status">待审批</view>
          </navigator>
        </view>
      </view>

      <view class="m-tab-pane" a:if="{{currentTabIndex == 1}}">
        <view class="m-adv-item" a:for="{{list}}">
            <navigator url="/pages/advance/advance?id={{item.id}}">
              <view class="m-adv-name">{{item.processTitle}}</view>
              <view class="m-adv-date">申请时间:{{item.date}}</view>
              <view class="m-adv-status green">审批通过</view>
            </navigator>
        </view>
      </view>

      <view class="loading" a:if="{{isLoading}}">
        <image mode="widthFix" src="data:image/gif;base64,"/>
        <text>努力加载中</text>
      </view>

      <view class="no-more-data" a:if="{{noData!='noMoreData'}}">
        <text>{{noData}}</text>
      </view>
  </view>
</view>